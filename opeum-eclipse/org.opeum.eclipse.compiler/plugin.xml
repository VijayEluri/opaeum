<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
 <extension
       point="org.eclipse.ui.startup">
    <startup
          class="org.opeum.eclipse.javasync.JavaTransformationProcessManager"></startup>
 </extension>
 <extension
       id="lkjl"
       point="org.opeum.eclipse.sourceFolderStrategy">
    <sourceFolderStrategy
          className="net.sf.opeum.pomgeneration.MultiProjectMavenSourceFolderStrategy">
    </sourceFolderStrategy>
    <sourceFolderStrategy
          className="net.sf.opeum.pomgeneration.SingleProjectMavenSourceFolderStrategy">
    </sourceFolderStrategy>
 </extension>
 <extension
       point="org.eclipse.ui.menus">
    <menuContribution
          allPopups="false"
          class="org.opeum.eclipse.starter.OpiumContributionFactory"
          locationURI="popup:org.eclipse.ui.popup.any?before=additions">
    </menuContribution>
 </extension>
 <extension
       point="org.opeum.eclipse.transformationStep">
    <transformationStep
          className="org.opeum.generation.features.PersistenceUsingHibernate">
    </transformationStep>
    <transformationStep
          className="org.opeum.generation.features.PersistenceUsingJpa">
    </transformationStep>
 </extension>
 <extension
       point="org.opeum.eclipse.modelLibrary">
    <modelLibrary
          name="Opium Simple Types"
          uri="pathmap://NAKEDUML_MODELS/libraries/OpiumSimpleTypes.library.uml">
    </modelLibrary>
    <modelLibrary
          name="Opium BPM and Business Calendar Library"
          uri="pathmap://NAKEDUML_MODELS/libraries/OpiumBPM.library.uml">
    </modelLibrary>
 </extension>
 <extension
       point="org.opeum.eclipse.profile">
    <profile
          name="Opium Standard Profile"
          uri="pathmap://NAKEDUML_MODELS/profiles/OpiumStandardProfile.uml">
    </profile>
    <profile
          name="Opium BPM Profile"
          uri="pathmap://NAKEDUML_MODELS/profiles/OpiumBPMProfile.uml">
    </profile>
    <profile
          name="javax.validation"
          uri="pathmap://NAKEDUML_MODELS/profiles/JavaxValidation.uml">
    </profile>
 </extension>
</plugin>
