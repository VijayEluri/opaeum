	<extension point="org.eclipse.ui.navigator.navigatorContent">
		<navigatorContent id="org.topcased.ui.navigator.uml.modelContent" name="UML Model File Contents"
			contentProvider="org.topcased.modeler.uml.internal.ui.navigator.UMLModelAdapterFactoryContentProvider" labelProvider="org.topcased.modeler.uml.internal.ui.navigator.UMLModelAdapterFactoryLabelProvider"
			activeByDefault="true" icon="icons/UML.gif" priority="normal">
