				<and>
					<instanceof value="org.eclipse.core.resources.IResource" />
					<test forcePluginActivation="true" property="org.eclipse.core.resources.extension" value="uml" />
				</and>
			</triggerPoints>
			<possibleChildren>
				<instanceof value="org.eclipse.emf.ecore.resource.Resource" />
			</possibleChildren>
