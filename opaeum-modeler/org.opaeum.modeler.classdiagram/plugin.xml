<?eclipse version="3.0"?>
<plugin>
	<extension-point id="operationBodyLanguage" name="Language for operation body" schema="schema/operationBodyLanguage.exsd" />
	<extension-point id="umlValidationExtension" name="umlValidationExtension" schema="schema/umlValidationExtension.exsd" />
	<extension-point id="extendedValidator" name="extendedValidator" schema="schema/extendedValidator.exsd" />
	<extension point="org.eclipse.ui.editors">
		<editor class="org.opaeum.topcased.uml.editor.OpaeumEditor" icon="icons/UML.gif" default="true" name="Opaeum Editor" id="org.topcased.modeler.uml.editor.UMLEditor"
			contributorClass="org.topcased.modeler.uml.actions.UMLEditorActionBarContributor" extensions="umldi,uml">
		</editor>
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="org.opaeum.eclipse.newWizards" name="Opaeum" />
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<wizard icon="icons/UML.gif" class="org.topcased.modeler.uml.wizards.NewUMLDiagrams" category="org.opaeum.eclipse.newWizards" name="Executable Model with Opaeum"
			id="org.topcased.modeler.uml.wizards.NewUMLDiagrams">
			<description>Create a new Executable Model with Opaeum</description>
			<selection class="org.eclipse.core.resources.IResource" />
		</wizard>
	</extension>
	<extension point="org.eclipse.emf.ecore.extension_parser">
		<parser type="umldi" class="org.topcased.modeler.diagrams.model.util.DiagramsResourceFactoryImpl" />
	</extension>
	<extension point="org.topcased.modeler.templates">
		<template di="templates/businessmodel/%name%.umldi" editorId="org.topcased.modeler.uml.editor.UMLEditor" id="org.topcased.modeler.uml.templates.common"
			isDiagram="false" model="templates/businessmodel/%name%.uml" name="Business Modeling" />
		<template di="templates/profilediagram/%name%.umldi" editorId="org.topcased.modeler.uml.editor.UMLEditor" id="org.topcased.modeler.uml.templates.profilediagram"
			isDiagram="false" model="templates/profilediagram/%name%.uml" name="Profile Template" />
	</extension>
	<extension point="org.eclipse.ui.perspectiveExtensions">
		<perspectiveExtension targetID="org.topcased.modeler.perspective">
			<newWizardShortcut id="org.topcased.modeler.uml.wizards.NewUMLDiagrams" />
		</perspectiveExtension>
	</extension>
	<extension point="org.eclipse.core.runtime.preferences">
		<initializer class="org.topcased.modeler.uml.alldiagram.preferences.AllDiagramPreferenceInitializer" />
	</extension>
