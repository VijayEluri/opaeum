<plugin>

   <extension point="org.topcased.modeler.templates">
      <template
            di="templates/org.nakeduml.uim.classform/%name%.uimdi"
            model="templates/org.nakeduml.uim.classform/%name%.uim"
            name="Class Form"
            id="org.nakeduml.uim.classform.templates.net.sf.nakeduml.uim.classform"
            editorId="org.nakeduml.uim.modeleditor.editor.UIMEditor"/>
   </extension>
   
   <extension point="org.topcased.modeler.diagrams">
      <diagram
            configuration="org.nakeduml.uim.classform.ClassFormConfiguration"
            id="org.nakeduml.uim.classform"
            name="Class Form">
         <enableFor
               class="org.nakeduml.uim.ClassForm">
         </enableFor>
         <enableFor
               class="org.nakeduml.uim.OperationInvocationForm">
         </enableFor>
         <enableFor
               class="org.nakeduml.uim.StateForm">
         </enableFor>
         <enableFor
               class="org.nakeduml.uim.ActionTaskForm">
         </enableFor>
         <enableFor
               class="org.nakeduml.uim.OperationTaskForm">
         </enableFor>
      </diagram>
	</extension>
	
	
