			<propertyTab id="org.opaeum.eclipse.extendedTab" category="domain" afterTab="org.opaeum.eclipse.opaeumTab"
				label="Extended" indented="true" />
			<propertyTab id="org.opaeum.eclipse.i8nTab" category="domain" afterTab="org.opaeum.eclipse.opaeumTab"
				label="Languages" indented="true" />
			<propertyTab id="org.opaeum.eclipse.firstEndTab" category="domain" afterTab="org.opaeum.eclipse.opaeumTab"
				label="First End" indented="true" />
			<propertyTab id="org.opaeum.eclipse.secondEndTab" category="domain" afterTab="org.opaeum.eclipse.firstEndTab"
				label="Second End" indented="true" />
				
			<propertyTab id="org.opaeum.eclipse.interfacesTab"
				category="domain" label="Interfaces" indented="true"
				afterTab="org.opaeum.eclipse.opaeumTab" />
