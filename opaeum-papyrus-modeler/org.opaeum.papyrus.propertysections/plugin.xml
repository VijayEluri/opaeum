				category="domain" label="Notification Text" afterTab="org.opaeum.eclipse.opaeumTab"
				indented="true" />
			<propertyTab category="appearance"
				id="property.tab.AppearancePropertySection" afterTab="org.opaeum.eclipse.opaeumTab"
				label="Appearance1" />
			<propertyTab category="domain"
				id="property.tab.DiagramPropertySection" afterTab="org.opaeum.eclipse.opaeumTab"
				label="Diagram" />
			<propertyTab id="property.tab.advanced" category="domain" 
				label="Advanced1" afterTab="org.opaeum.eclipse.opaeumTab"
				indented="false" />
   		<propertyTab id="Base" label="General" category="org.eclipse.papyrus" afterTab=""/>
