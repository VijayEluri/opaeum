            class="org.opaeum.disablement.Startup">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.opaeum.disablement.Activate"
            commandId="org.opaeum.papyrus.disablement.command1">
      </handler>
   </extension>
   <extension
         id="deaasdfasdf"
         name="Deactivate DEX"
         point="org.eclipse.ui.commands">
