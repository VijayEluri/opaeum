            id="org.opaeum.papyrus.disablement.activity"
            name="Crap To Disable">
      </activity>
      <activityPatternBinding
            activityId="org.opaeum.papyrus.disablement.activity"
            isEqualityPattern="false"
            pattern="org\.eclipse\.datatools\..*/.*">
      </activityPatternBinding>
      <category
            id="org.opaeum.papyrus.disablement.category"
            name="Opaeum">
      </category>
      <categoryActivityBinding
            activityId="org.opaeum.papyrus.disablement.activity"
