<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.opaeum.uimodeler.feature"
      label="Opaeum User Interface Modeler Feature"
      version="1.0.0.qualifier"
      provider-name="Opaeum">

   <description url="%license">
      Opaeum User Interface Modeler provides graphical editors for User Interfaces that ties in directly with the for UML2 models.
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.uml2.uml.edit"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.draw2d"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.emf.edit.ui"/>
      <import plugin="org.eclipse.emf.ecore.edit"/>
      <import plugin="org.eclipse.uml2.uml"/>
      <import plugin="org.topcased.modeler" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.views.properties.tabbed"/>
      <import plugin="org.topcased.tabbedproperties"/>
      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import feature="org.opaeum.metamodels.feature" version="1.0.0.qualifier"/>
      <import plugin="org.opaeum.metamodels.uim"/>
      <import plugin="org.opaeum.metamodels.emfworkspace"/>
   </requires>

   <plugin
         id="org.opaeum.uimodeler.classform"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.uimodeler.edit"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.uimodeler.editor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.uimodeler.modeleditor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.uimodeler.userinteractionproperties"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
