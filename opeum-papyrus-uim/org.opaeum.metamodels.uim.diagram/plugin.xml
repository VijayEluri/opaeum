            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="uim_diagram"/>
               </and>
               <instanceof value="org.opaeum.uim.diagram.navigator.UimAbstractNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <or>
         	   <instanceof value="org.opaeum.uim.diagram.navigator.UimAbstractNavigatorItem"/>
            </or>
         </possibleChildren>
         <commonSorter 
               id="org.opaeum.metamodels.uim.diagram.navigatorSorter" 
               class="org.opaeum.uim.diagram.navigator.UimNavigatorSorter">
            <parentExpression>
               <or>
	              <and>
    	             <instanceof value="org.eclipse.core.resources.IFile"/>
        	         <test property="org.eclipse.core.resources.extension" value="uim_diagram"/>
                  </and>
                  <instanceof value="org.opaeum.uim.diagram.navigator.UimAbstractNavigatorItem"/>
               </or>
            </parentExpression>
         </commonSorter>
      </navigatorContent>
      <navigatorContent
            id="org.opaeum.metamodels.uim.diagram.domainResourceContent" 
            name="%domainNavigatorContentName" 
            priority="normal" 
            contentProvider="org.opaeum.uim.diagram.navigator.UimDomainNavigatorContentProvider" 
            labelProvider="org.opaeum.uim.diagram.navigator.UimDomainNavigatorLabelProvider"
            icon="icons/obj16/EditorDiagramFile.gif"
            activeByDefault="true">
         <triggerPoints>
            <or>
	           <and>
    	          <instanceof value="org.eclipse.core.resources.IFile"/>
        	      <test property="org.eclipse.core.resources.extension" value="uim"/>
               </and>
               <instanceof value="org.opaeum.uim.diagram.navigator.UimDomainNavigatorItem"/>
            </or>
         </triggerPoints>
         <possibleChildren>
            <instanceof value="org.opaeum.uim.diagram.navigator.UimDomainNavigatorItem"/>
         </possibleChildren>
      </navigatorContent>
      <actionProvider
            id="org.opaeum.metamodels.uim.diagram.navigatorActionProvider"
