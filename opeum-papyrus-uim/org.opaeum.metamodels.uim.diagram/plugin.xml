            id="org.opaeum.metamodels.uim.diagram.resourceContent" 
            name="%navigatorContentName" 
            priority="normal" 
            contentProvider="org.opaeum.uim.diagram.navigator.UimNavigatorContentProvider" 
            labelProvider="org.opaeum.uim.diagram.navigator.UimNavigatorLabelProvider"
            icon="icons/obj16/EditorDiagramFile.gif"
            activeByDefault="true">
