<?xml version="1.0" encoding="UTF-8"?>
<query:ModelQuerySet
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:query="http://www.eclipse.org/EmfFacet/infra/query/0.8.incubation"
    name="UimBrowserQuery">
  <associatedMetamodels
      href="http://www.eclipse.org/gmf/runtime/1.0.2/notation#/"/>
  <associatedMetamodels
      href="http://www.eclipse.org/emf/2002/Ecore#/"/>
  <associatedMetamodels
      href="http://www.eclipse.org/uml2/4.0.0/Types#/"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//action"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//binding"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//constraint"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//control"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//editor"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//panel"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//perspective"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#/"/>
  <associatedMetamodels
      href="http://opaeum.org/uimetamodel/1.0#//wizard"/>
  <queries xsi:type="query:JavaModelQuery"
      name="IsReadOnly"
      description="Return true if the resource is in a read only mode"
      implementationClassName="org.opaeum.uimodeler.modelexplorer.queries.IsReadOnly">
    <returnType
        xsi:type="ecore:EDataType"
        href="http://www.eclipse.org/uml2/4.0.0/Types#//Boolean"/>
    <scope href="http://www.eclipse.org/gmf/runtime/1.0.2/notation#//Diagram"/>
    <scope href="http://opaeum.org/uimetamodel/1.0#//editor/EditorPage"/>
  </queries>
  <queries xsi:type="query:JavaModelQuery"
      name="IsUimDiagramContainer"
      description="return true if the element contains diagrams"
      implementationClassName="org.opaeum.uimodeler.modelexplorer.queries.IsUimDiagramContainer">
    <returnType
        xsi:type="ecore:EDataType"
        href="http://www.eclipse.org/uml2/4.0.0/Types#//Boolean"/>
    <scope href="http://opaeum.org/uimetamodel/1.0#//UserInteractionElement"/>
  </queries>
  <queries xsi:type="query:JavaModelQuery"
      name="GetContainedUimDiagrams"
      description="Get the collection of all contained diagrams"
      upperBound="-1"
      implementationClassName="org.opaeum.uimodeler.modelexplorer.queries.GetContainedUimDiagrams">
    <returnType
        xsi:type="ecore:EClass"
        href="http://www.eclipse.org/gmf/runtime/1.0.2/notation#//Diagram"/>
    <scope href="http://opaeum.org/uimetamodel/1.0#//UserInteractionElement"/>
  </queries>
  <queries xsi:type="query:JavaModelQuery"
      name="GetUimDiagramIcon"
      description="Return the path to the icon of the corresponding diagram"
      implementationClassName="org.opaeum.uimodeler.modelexplorer.queries.GetUimDiagramIcon">
    <returnType
        xsi:type="ecore:EDataType"
        href="http://www.eclipse.org/uml2/4.0.0/Types#//String"/>
    <scope href="http://www.eclipse.org/gmf/runtime/1.0.2/notation#//Diagram"/>
  </queries>
  <queries xsi:type="query:JavaModelQuery"
      name="IsUimDiagram"
      description="Return true if the element is a diagram"
      implementationClassName="org.opaeum.uimodeler.modelexplorer.queries.IsUimDiagram">
    <returnType
        xsi:type="ecore:EDataType"
        href="http://www.eclipse.org/uml2/4.0.0/Types#//Boolean"/>
    <scope href="http://www.eclipse.org/gmf/runtime/1.0.2/notation#//Diagram"/>
  </queries>
</query:ModelQuerySet>
