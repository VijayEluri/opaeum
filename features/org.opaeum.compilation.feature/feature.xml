<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.opaeum.compilation.feature"
      label="Opaeum Model Compilation Feature"
      version="1.0.2.qualifier"
      provider-name="opaeum.org">

   <description url="%license">
      Opaeum Compilation Feature provides transformation algorithms
from the Opaeum Metamodel to the various implementation platforms.
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.opaeum.metamodels.text"/>
      <import plugin="org.opaeum.metamodels.java"/>
      <import plugin="org.opaeum.metamodels.emfworkspace"/>
      <import plugin="org.opaeum.metamodels.maven"/>
      <import feature="org.opaeum.metamodels.feature" version="1.0.2.qualifier"/>
      <import plugin="org.eclipse.uml2.uml"/>
      <import plugin="org.eclipse.ocl.uml"/>
      <import plugin="org.eclipse.ocl"/>
      <import plugin="org.opaeum.metamodels.java" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.opaeum.metamodels.emfworkspace" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore"/>
   </requires>

   <plugin
         id="org.opaeum.compilation.filegeneration"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.compilation.java"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.compilation.tinker"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.compilation.misc"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.compilation.ocltojava"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.opaeum.transformation.engine"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
